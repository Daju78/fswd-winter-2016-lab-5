    "express-session": "^1.13.0",
    "jade": "^1.11.0",
    "nodemon": "^1.9.2",
    "pg": "^4.4.6",
    "sequelize": "^3.19.3",
    "sequelize-cli": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "express-debug": "^1.1.1",
